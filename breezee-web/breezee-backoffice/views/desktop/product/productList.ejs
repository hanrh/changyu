<div class="page-header">
    <div class="pull-left">
        <h1 class="content-title">产品管理</h1>
    </div>
</div>
<div class="dolphin-col-24 main" id="myContent">
    <div class="dolphin-row">
        <div class="dolphin-col-24">
            <div class="panel panel-primary box">
                <div class="panel-heading box-title">
                    <h3 class="panel-title">
                        <i class="fa fa-table fa-lg" aria-hidden="true"></i>
                        产品列表
                    </h3>
                    <div class="actions">
                        <a class="btn btn-mini content-refresh" href="#">
                            <i class="fa fa-refresh"></i>
                        </a>
                    </div>
                </div>
                <div id="prtList"></div>
            </div>
        </div>
    </div>
</div>

<div class="modal" id="product_win">
    <div class="modal-dialog" style="width: 60%;left: 20%;top:0;">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"
                        aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                <span class="modal-title">产品信息</span>
            </div>
            <div class="modal-body">
                <div>
                    <!-- Nav tabs -->
                    <ul class="nav nav-tabs" role="tablist">
                        <li role="presentation" class="active">
                            <a href="#home" aria-controls="home" role="tab" data-toggle="tab">基本信息</a>
                        </li>
                        <li role="presentation">
                            <a href="#image" aria-controls="image" role="tab" data-toggle="tab">产品图片</a>
                        </li>
                        <li role="presentation">
                            <a href="#price" aria-controls="price" role="tab" data-toggle="tab">产品价格</a>
                        </li>
                        <li role="presentation">
                            <a href="#profile" aria-controls="profile" role="tab" data-toggle="tab">产品限售</a>
                        </li>
                    </ul>
                    <!-- Tab panes -->
                    <div class="tab-content"
                         style="max-height: 600px;min-height:500px;overflow: auto;overflow-x: hidden;">
                        <div role="tabpanel" class="tab-pane active" id="home">
                            <form class="form-horizontal edit-form">
                                <div class="form-group">
                                    <label class="col-md-offset-1 col-md-2 control-label">产品编码</label>
                                    <div class="col-md-6">
                                        <input class="form-control" name="code" type="text">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-offset-1 col-md-2 control-label">简单描述</label>
                                    <div class="col-md-6">
                                        <textarea name="shortText" style="width: 100%;height: 100px;"></textarea>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-offset-1 col-md-2 control-label">品牌</label>
                                    <div class="col-md-6">
                                        <input class="form-control" name="brand" type="text">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-offset-1 col-md-2 control-label">交货工厂</label>
                                    <div class="col-md-6">
                                        <input class="form-control" name="factory" type="text">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-offset-1 col-md-2 control-label">&nbsp;箱因子</label>
                                    <div class="col-md-6">
                                        <input class="form-control" name="karDivisor" type="text">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-offset-1 col-md-2 control-label">&nbsp;托因子</label>
                                    <div class="col-md-6">
                                        <input class="form-control" name="palDivisor" type="text">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-offset-1 col-md-2 control-label">物料组</label>
                                    <div class="col-md-6">
                                        <input class="form-control" name="group" type="text">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-offset-1 col-md-2 control-label">计量单位</label>
                                    <div class="col-md-6">
                                        <input class="form-control" name="measureUnit" type="text">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-offset-1 col-md-2 control-label">销售单位</label>
                                    <div class="col-md-6">
                                        <input class="form-control" name="saleUnit" type="text">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-offset-1 col-md-2 control-label">产品毛重</label>
                                    <div class="col-md-6">
                                        <input class="form-control" name="grossWeight" type="text">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-offset-1 col-md-2 control-label">产品净重</label>
                                    <div class="col-md-6">
                                        <input class="form-control" name="netWeight" type="text">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-offset-1 col-md-2 control-label">产品体积</label>
                                    <div class="col-md-6">
                                        <input class="form-control" name="bulk" type="text">
                                    </div>
                                </div>
                            </form>
                        </div>
                        <div role="tabpanel" class="tab-pane" id="image">
                        </div>
                        <div role="tabpanel" class="tab-pane" id="price">
                            <div id="priceList"></div>
                        </div>
                        <div role="tabpanel" class="tab-pane" id="profile">
                            <div id="limitList"></div>
                        </div>

                    </div>
                </div>

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
<script src="<%= global.config.publicPath %>/page/scripts/<%= path %>.js"></script>